{
    "name": "bonlineza/laravel-typed-state-machine",
    "description": "Laravel wrapper around bonlineza/typed-state-machine",
    "type": "library",
    "require": {
        "laravel/framework": "~6.0",
        "bonlineza/typed-state-machine": "^1.0",
        "bagf/dynamic": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "authors": [
        {
            "name": "countnoobula",
            "email": "shaunmichaelvantonder@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TypedStateMachine\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TypedStateMachine\\Laravel\\TypedStateMachineServiceProvider"
            ]
        }
    }
}
